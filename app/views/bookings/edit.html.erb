<div class="container">
<h1>Edit Booking</h1>
<%= simple_form_for @booking do |f| %>
  <%= f.input :start_date,  as: :date, html5: true,  label: 'CHECK-IN' %>
  <%= f.input :end_date ,  as: :date, html5: true,  label: 'CHECK-OUT'  %>

  <%= f.submit "Reserve", class: "btn btn-primary" %>
<% end %>
</div>
